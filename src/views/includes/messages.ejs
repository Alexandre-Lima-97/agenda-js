<% if (errors.length > 0) { %>
<div class="row">
        <div class="col my-3">
                <div class="alert alert-danger">
                        <% console.log(errors); %>
                        <% errors.forEach(erro => { %>
                                <%= erro%><br> 
                       <% }); %>
                </div>
              </div>
</div>

<% } %>

<% if (success.length > 0) { %>
        <div class="row">
                <div class="col my-3">
                        <div class="alert alert-succes">
                                <% console.log(success); %>
                                <% success.forEach(succes => { %>
                                        <%= succes%><br> 
                               <% }); %>
                        </div>
                      </div>
        </div>
        
        <% } %>